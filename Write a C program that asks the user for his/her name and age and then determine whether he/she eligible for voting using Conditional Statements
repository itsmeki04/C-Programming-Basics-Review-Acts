#include <stdio.h>
#include <stdlib.h>

int main()
{
       char name[30];
       int age;
 
        printf("name: ");
        scanf("%[^\n]s", name);
        printf("age: ");
        scanf("%d", &age);

       printf("name: %s\n", name);
       printf("age: %d", age);

       if(age == NULL){
          printf("\n\nInvalid Age, Please try again");
    }
       if( age >= 18){
          printf("\n\nYou are eligible for voting");
    }
      else if( age <= 17){
          printf("\n\nYou are not eligible for voting");
    }

     return 0;

}
